{
  "name": "beerimanager",
  "version": "1.0.0",
  "description": "Parent Committee Coordination App - A Hebrew-first PWA for managing school parent committee activities",
  "main": "index.js",
  "scripts": {
    "dev": "lsof -ti:4500 | xargs kill -9 2>/dev/null || true && next dev -p 4500",
    "dev:clean": "lsof -ti:4500 | xargs kill -9 2>/dev/null || true && rm -rf .next && next dev -p 4500",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "playwright test",
    "test:ui": "playwright test --ui",
    "test:debug": "playwright test --debug",
    "test:headed": "playwright test --headed",
    "test:mobile": "playwright test --project='Mobile Chrome'",
    "test:performance": "playwright test tests/e2e/05-performance.spec.ts",
    "test:parallel": "playwright test --workers=4",
    "test:report": "playwright show-report",
    "automation:watch": "playwright test --watch",
    "automation:dev": "concurrently \"npm run dev\" \"npm run automation:watch\"",
    "db:start": "./scripts/db-commands.sh start",
    "db:stop": "./scripts/db-commands.sh stop",
    "db:migrate": "./scripts/db-commands.sh migrate-all",
    "db:reset": "./scripts/db-commands.sh reset",
    "db:backup": "./scripts/db-commands.sh backup",
    "db:connect": "./scripts/db-commands.sh connect",
    "db:export-schema": "./scripts/export-schema.sh",
    "db:import-schema": "./scripts/import-schema.sh",
    "db:compare-schemas": "./scripts/compare-schemas.sh",
    "type-check": "tsc --noEmit",
    "icons:generate": "node scripts/generate-icons.js",
    "postinstall": "npm run icons:generate"
  },
  "keywords": [
    "pwa",
    "parent-committee",
    "hebrew",
    "calendar",
    "nextjs"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@hookform/resolvers": "^3.3.0",
    "@radix-ui/react-accordion": "^1.2.12",
    "@radix-ui/react-alert-dialog": "^1.1.15",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-checkbox": "^1.3.3",
